#include<stdio.h>
int main()
{
    int n,m,i;
    printf("Enter the number of array element");
    scanf("%d",&n);
    int a[n];
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter the new number of array element: ");
    scanf("%d",&m);
    int b[m];
    for(i=0;i<m;i++)
    {
        scanf("%d",&b[i]);
    }
    printf("the new array is\n");
    for(i=0;i<n;i++)
    {
        printf("%d\t",a[i]);
    }
    for(i=0;i<m;i++)
    {
        printf("%d\t",b[i]);
    }
}
